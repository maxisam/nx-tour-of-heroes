{
  "version": 1,
  "projects": {
    "demo-one": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/demo-one",
      "sourceRoot": "apps/demo-one/src",
      "prefix": "nx-demo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-one/browser",
            "index": "apps/demo-one/src/index.html",
            "main": "apps/demo-one/src/main.ts",
            "polyfills": "apps/demo-one/src/polyfills.ts",
            "tsConfig": "apps/demo-one/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/demo-one/src/favicon.ico",
              "apps/demo-one/src/assets",
              {
                "glob": "**/*",
                "input": "../libs/shared/theme",
                "output": "./assets/"
              }
            ],
            "styles": ["apps/demo-one/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/theme/src/lib/styles"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo-one/src/environments/environment.ts",
                  "with": "apps/demo-one/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-one:build",
            "proxyConfig": "apps/demo-one/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-one:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-one:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/demo-one/tsconfig.app.json", "apps/demo-one/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/demo-one/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/demo-one/jest.config.js",
            "tsConfig": "apps/demo-one/tsconfig.spec.json",
            "setupFile": "apps/demo-one/src/test-setup.ts"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/demo-one/server",
            "main": "apps/demo-one/server.ts",
            "tsConfig": "apps/demo-one/tsconfig.server.json",
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/theme/src/lib/styles"]
            },
            "externalDependencies": [
              "@nestjs/microservices",
              "@nestjs/microservices/microservices-module",
              "@nestjs/websockets",
              "@nestjs/websockets/socket-module",
              "cache-manager",
              "class-transformer",
              "class-validator"
            ]
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/demo-one/src/environments/environment.ts",
                  "with": "apps/demo-one/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": false
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "demo-one:build",
            "serverTarget": "demo-one:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-one:build:production",
              "serverTarget": "demo-one:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "demo-one:build:production",
            "serverTarget": "demo-one:server:production",
            "routes": ["/demo/page-one", "/demo/page-two"]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/api/tsconfig.app.json", "apps/api/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json"
          }
        }
      }
    },
