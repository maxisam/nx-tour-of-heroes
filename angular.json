        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/shared-ui",
            "config": {
              "configFolder": ".storybook"
            }
          },
          "configurations": {
            "ci": {
