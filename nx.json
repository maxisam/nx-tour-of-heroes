{
  "npmScope": "nx-demo",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": "*",
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "build-storybook"]
      }
    }
  },
  "projects": {
    "api": {
      "tags": ["scope:api", "type:api"]
    },
    "api-data-access": {
      "tags": ["scope:api", "type:api"]
    },
    "demo-one": {
      "tags": ["scope:demo-one", "type:app"]
    },
    "demo-one-feature-page-one": {
      "tags": ["scope:demo-one", "type:feature"]
    },
    "demo-one-feature-page-two": {
      "tags": ["scope:demo-two", "type:feature"]
    },
    "demo-one-feature-shell": {
      "tags": ["scope:demo-one", "type:feature"]
    },
    "shared-animations": {
      "tags": ["scope:shared", "type:animations"]
    },
    "shared-data-access": {
      "tags": ["scope:shared", "type:data-access"]
    },
    "shared-models": {
      "tags": ["scope:shared", "type:types"]
    },
    "shared-theme": {
      "tags": ["type:theme"]
    },
    "shared-ui": {
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-utils": {
      "tags": ["type:utils"]
    },
    "tour-of-heroes": {
      "tags": ["scope:tour-of-heroes", "type:app"]
    },
    "tour-of-heroes-heroes-feature": {
      "tags": ["scope:tour-of-heroes", "type:feature"]
    },
    "tour-of-heroes-heroes-data-access": {
      "tags": ["scope:tour-of-heroes", "type:data-access"]
    }
  }
}
