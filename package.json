{
  "name": "nx-tour-of-heroes",
  "version": "10.2.0-alpha.2",
  "author": "Scott Robertson (https://github.com/sc0tt5)",
  "license": "SEE LICENSE",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "concurrently --names 'JSONDB,NESTJS,ANGULAR' --prefix-colors 'blue,green,yellow,' --kill-others \"npm:serve:database\"  \"npm:serve:api\" \"npm:serve\" ",
    "serve": "ng serve --configuration=local --open --watch",
    "build": "ng build",
    "test": "npx jest --coverage",
    "lint": "nx workspace-lint && ng lint",
    "doc:api": "compodoc -p apps/api/tsconfig.json -d tmp/documentation/api -s -o",
    "doc:tour-of-heroes": "compodoc -p apps/tour-of-heroes/tsconfig.json -d tmp/documentation/tour-of-heroes -s -o",
    "lighthouse": "ts-node tools/scripts/lighthouse.run.ts",
    "analyze:bundles": "ng run tour-of-heroes:analyze --diffLoading=false",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "serve:api": "ng serve api",
    "serve:database": "json-server --watch apps/tour-of-heroes/db.json --port 8800 --middlewares ./node_modules/json-server-reset",
    "build:storybook": "build-storybook -o dist/storybook",
    "serve:storybook": "start-storybook",
    "dev:ssr": "concurrently --names 'JSONDB,NESTJS' --prefix-colors 'blue,yellow,' --kill-others \"npm:serve:database\" \"ng run tour-of-heroes:serve-ssr:local\" ",
    "build:ssr": "ng build --prod && ng run tour-of-heroes:server:production",
    "serve:ssr": "concurrently --names 'JSONDB,SERVER' --prefix-colors 'blue,yellow,' --kill-others \"npm:serve:database\" \"node dist/tour-of-heroes/server/main.js\"",
    "prerender": "ng run tour-of-heroes:prerender",
    "prebuild:ssr": "ngcc",
    "postinstall": "node ./tools/scripts/decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "sideEffects": [
    "**/main.ts",
    "**/main.server.ts",
    "**/polyfills.ts"
  ],
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,html,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.5",
    "@angular/common": "10.2.5",
    "@angular/compiler": "10.2.5",
    "@angular/core": "10.2.5",
    "@angular/forms": "10.2.5",
    "@angular/platform-browser": "10.2.5",
    "@angular/platform-browser-dynamic": "10.2.5",
    "@angular/platform-server": "10.2.5",
    "@angular/router": "10.2.5",
    "@nestjs/common": "7.6.15",
    "@nestjs/core": "7.6.15",
    "@nestjs/ng-universal": "4.3.0",
    "@nestjs/platform-express": "7.6.15",
    "@ngrx/effects": "10.1.2",
    "@ngrx/entity": "10.1.2",
    "@ngrx/router-store": "10.1.2",
    "@ngrx/store": "10.1.2",
    "@ngrx/store-devtools": "10.1.2",
    "@nrwl/angular": "10.4.15",
    "core-js": "3.10.1",
    "ngx-logger": "4.2.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1002.3",
    "@angular/cli": "10.2.3",
    "@angular/compiler-cli": "10.2.5",
    "@angular/language-service": "10.2.5",
    "@compodoc/compodoc": "1.1.11",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.15",
    "@nguniversal/builders": "10.1.0",
    "@ngx-builders/analyze": "2.1.0",
    "@nrwl/cli": "10.4.15",
    "@nrwl/jest": "10.4.15",
    "@nrwl/nest": "10.4.15",
    "@nrwl/node": "10.4.15",
    "@nrwl/storybook": "10.4.15",
    "@nrwl/tao": "10.4.15",
    "@nrwl/workspace": "10.4.15",
    "@storybook/addon-knobs": "6.2.8",
    "@storybook/angular": "6.2.8",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/json-server": "0.14.3",
    "@types/node": "14.14.39",
    "@types/webpack": "5.28.0",
    "chrome-launcher": "0.13.4",
    "codelyzer": "6.0.1",
    "concurrently": "6.0.2",
    "dotenv": "8.2.0",
    "eslint": "7.12.1",
    "express": "4.17.1",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "jest-html-reporters": "2.1.3",
    "jest-marbles": "2.5.1",
    "jest-preset-angular": "8.4.0",
    "json-server": "0.16.3",
    "json-server-reset": "1.3.0",
    "lighthouse": "7.3.0",
    "lint-staged": "10.5.4",
    "ngrx-store-freeze": "0.2.4",
    "ora": "5.4.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "3.9.5"
  },
  "engines": {
    "node": ">=12.11.0 <13.0.0"
  }
}
